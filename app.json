{
  "expo": {
    "name": "bodymech-client-frontend",
    "slug": "bodymech-client-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "ios",
      "web"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wylerzahm.bodymech",
      "googleServicesFile": "GoogleService-Info.plist",
      "minVersion": "12.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "google-services.json",
      "package": "com.wylerzahm.bodymech"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics",
      "expo-secure-store",
      "expo-image-picker",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "faceIdPermissions": "Allow us to access your Face ID.",
            "photosPermissions": "Allow us to access your photos."
          }
        }
      ],
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "apiUrl": "https://api.yourapp.com",
      "useSampleData": true,
      "firebaseConfig": {
        "FIREBASE_API_KEY": "AIzaSyDDoY2mB3tR2J-QbwihPJLoSH3ya9CBRrY",
        "FIREBASE_AUTH_DOMAIN": "mech-2b0ad.firebaseapp.com",
        "FIREBASE_PROJECT_ID": "mech-2b0ad",
        "FIREBASE_STORAGE_BUCKET": "mech-2b0ad.appspot.com",
        "FIREBASE_MESSAGING_SENDER_ID": "396836920221",
        "FIREBASE_APP_ID": "1:396836920221:web:ff4b5759f0dad6960fed74",
        "FIREBASE_MEASUREMENT_ID": "G-SE4L1C13PR",
        "FIREBASE_DATABASE_URL": "https://default.nam5.firebasedatabase.app"
      }
    },
    "entryPoint": "./index.js"
  }
}
